                <xs:attribute type="xs:string" name="note" use="optional"/>
                <xs:attribute type="xs:string" name="tp" use="optional"/>
                <xs:attribute type="xs:string" name="ts" use="optional"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>
